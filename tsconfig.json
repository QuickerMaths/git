{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "Node LTS",
  "_version": "20.1.0",
  "compilerOptions": {
    "lib": [
      "es2023"
    ],
    "allowImportingTsExtensions": true,
    "noEmit": true,
    "outDir": "dist",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "target": "es5", 
    "module": "commonjs",
    "moduleResolution": "node"
  },
  "include": ["src"]
}
